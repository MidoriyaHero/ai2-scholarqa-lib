{
  "logs": {
    "log_dir": "logs",
    "log_level": "INFO",
    "llm_cache_dir": "llm_cache",
    "event_trace_loc": "scholarqa_traces",
    "tracing_mode": "local"
  },
  "run_config": {
    "retrieval_service": "localhost",
    "retriever_args": {
      "n_retrieval": 256,
      "n_keyword_srch": 20,
      "base_url": "http://localhost:8080/api/v1/"
    },
    "reranker_service": "modal",
    "reranker_args": {
      "app_name": "ai2-scholar-qa-update",
      "api_name": "inference_api",
      "batch_size": 256,
      "gen_options": {}
    },
    "paper_finder_args": {
      "n_rerank": 50,
      "context_threshold": 0.0
    },
    "pipeline_args": {
      "validate": true,
      "llm_model": "gemini/gemini-2.5-flash-preview-04-17",
      "fallback_llm": "gemini/gemini-2.5-flash-preview-04-17",
      "decomposer_llm": "gemini/gemini-2.5-flash-preview-04-17"
    }
  }
}